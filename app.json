{
  "expo": {
      "name": "planner",
      "slug": "event_planner",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/react-logo.png",
      "scheme": "planner-app",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": false,
      "ios": {
          "supportsTablet": true,
          "config": {
              "googleMapsApiKey": "AIzaSyCnfhl971CkXOZCNd4jQDu1_MCUJWSQjTo"
          },
          "infoPlist": {
              "NSCameraUsageDescription": "Разрешите доступ к камере. Она используется для возможности сфотографировать Ваше судно.",
              "NSAppTransportSecurity": {
                  "NSAllowsArbitraryLoads": true,
                  "NSPhotoLibraryUsageDescription": "Разрешите доступ к галерее. Она используется для загрузки фотографий Вашего судна.",
                  "NSCameraUsageDescription": "Разрешите доступ к камере. Она используется для возможности сфотографировать Ваше судно."
              }
          }
      },
      "android": {
          "adaptiveIcon": {
              "foregroundImage": "./assets/images/react-logo.png",
              "backgroundColor": "#ffffff"
          },
          "package": "com.event_planner",
          "config": {
              "googleMaps": {
                  "apiKey": "AIzaSyCnfhl971CkXOZCNd4jQDu1_MCUJWSQjTo"
              }
          },
          "permissions": [
              "android.permission.WRITE_EXTERNAL_STORAGE",
              "android.permission.CAMERA",
              "android.permission.READ_EXTERNAL_STORAGE",
              "android.permission.ACCESS_COARSE_LOCATION",
              "android.permission.ACCESS_FINE_LOCATION",
              "android.permission.INTERNET"
          ]
      },

      "web": {
          "bundler": "metro",
          "output": "static",
          "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
          "expo-router",
          [
              "expo-splash-screen",
              {
                  "image": "./assets/images/react-logo.png",
                  "imageWidth": 200,
                  "resizeMode": "contain",
                  "backgroundColor": "#ffffff"
              }
          ],
          [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25",
            "gradleVersion": "8.13",
            "agpVersion": "8.0.1",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ]
         
      ],
      "experiments": {
          "typedRoutes": true
      }
  }
}
